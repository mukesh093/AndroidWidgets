<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bridgelabz.com.android_widgets">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".button"/>
        <activity android:name=".editText"/>
        <activity android:name=".autoCompleteTextView"/>
        <activity android:name=".toggleButton"/>
        <activity android:name=".datePicker"/>
        <activity android:name=".timePicker"/>
        <activity android:name=".progressBar"/>
    </application>

</manifest>